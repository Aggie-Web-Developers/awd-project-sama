!DOCTYPE html>
<html lang="en">
<%- include('partials/header') %>

	<body>
		<%- include('partials/nav') %>

		<div id="content" class="container login card">
			<form class="login-form">
                <h3 class="mt-5 mb-4" id="logintitle">LOGIN</h3> 
				<div class="form-group">
				  <label for="exampleInputEmail1" id="loginform">Email address</label>
				  <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
				  <small id="emailHelp" class="form-text text-muted"></small>
				</div>
				<div class="form-group">
				  <label for="exampleInputPassword1" id="loginform">Password</label>
				  <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
				</div>
				<button type="submit" class="btn btn-primary mb-5" id="loginform">Submit</button>
            </form>
		</div>

		<script src="/scripts/template.js"></script>
		<%- include('partials/footer') %>
	</body>

</html>
